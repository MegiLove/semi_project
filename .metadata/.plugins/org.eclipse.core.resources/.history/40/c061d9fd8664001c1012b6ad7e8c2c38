package com.sist.action;
import java.io.IOException;
import java.util.ArrayList;

import javax.servlet.ServletException;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import com.sist.dao.LocationDAO;
import com.sist.vo.LocationVO;
public class showLocAction implements SistAction {
	
	@Override
	public String proRequest(HttpServletRequest request, HttpServletResponse response)
			throws ServletException, IOException {
		
		LocationDAO dao = new LocationDAO();
		ArrayList<LocationVO> list = dao.listAll();
		
		request.setAttribute("list", list);
		request.setAttribute("province_id", province_id);
		request.setAttribute("province_db", province_db);
		request.setAttribute("province_show", province_show);
		System.out.println("minhaeng");
		return "showLoc.jsp";
	}
	
}
